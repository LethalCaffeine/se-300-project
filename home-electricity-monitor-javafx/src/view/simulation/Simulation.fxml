<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>

<BorderPane prefHeight="555.0" prefWidth="705.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.simulation.SimulationController">
   <center>
      <Pane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <ImageView fx:id="image" fitHeight="150.0" fitWidth="200.0" layoutX="154.0" layoutY="203.0" onMouseClicked="#imageClick" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../images/washing-machine.png" />
               </image>
            </ImageView>
            <ImageView fx:id="imageb" fitHeight="150.0" fitWidth="200.0" layoutX="353.0" layoutY="203.0" onMouseClicked="#imageClick" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../images/oven.png" />
               </image>
            </ImageView>
            <Button fx:id="simulation" layoutX="279.0" layoutY="389.0" mnemonicParsing="false" onAction="#imageClick" prefHeight="61.0" prefWidth="107.0" text="Run simulation" />
            <Label fx:id="timeLabel" alignment="CENTER" layoutX="243.0" layoutY="98.0" prefHeight="81.0" prefWidth="180.0" text="00:00:00" textAlignment="CENTER" />
         </children>
      </Pane>
   </center>
</BorderPane>
